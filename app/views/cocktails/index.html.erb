<div class="wrapper-card">
  <div class="container">
    <div class="row">
      <% @cocktails.each do |cocktail| %>
        <div class="col-md-6">
          <div class="cocktail text-center">
            <h2><%= link_to "🍹", cocktail_path(cocktail), class: 'link' %> <%= cocktail.name %></h2>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>

